{
	"include": ["src/**/*","src/*"],
	"exclude": [
		"./public_html/**"
	  ],
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"sourceMap": true,
		"outDir": "./tmp/",
		"noImplicitAny": true,
		"module": "ES2020",
		"target": "ES2020",
		"strict": true,
		"allowJs": true,
		"declaration": false,
		"moduleResolution": "node16",
		"typeRoots":["src/global.d.ts"]
	}
}
